

local function generateRandomString(length)
    local charSet = {}
    for i = 48, 57 do -- ASCII values for numbers 0-9
        table.insert(charSet, string.char(i))
    end
    for i = 65, 90 do -- ASCII values for uppercase letters A-Z
        table.insert(charSet, string.char(i))
    end
    for i = 97, 122 do -- ASCII values for lowercase letters a-z
        table.insert(charSet, string.char(i))
    end

    local randomString = ""
    for _ = 1, length do
        local randomIndex = math.random(1, #charSet)
        randomString = randomString .. charSet[randomIndex]
    end
    return randomString
end

local randomString = generateRandomString(10)

print("Zylic: " .. randomString)
